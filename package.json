{
    "name": "jutil",
    "description": "Command-line utilities for manipulating JSON",
    "homepage": "http://github.com/misterfifths/jutil",
    "version": "0.1.1",
    "author": "Tim Clem",
    "license": "MIT",
    "preferGlobal": true,
    "repository": {
        "type": "git",
        "url": "http://github.com/misterfifths/jutil.git"
    },
    "files": [
        "*.js",
        "subcommands"
    ],
    "main": "./jutil",
    "bin": {
        "jutil": "./jutil.js",
        "jwhere": "./jutil.js",
        "jfirst": "./jutil.js",
        "jcount": "./jutil.js",
        "jselect": "./jutil.js",
        "jprops": "./jutil.js",
        "jformat": "./jutil.js",
        "jsort": "./jutil.js",
        "jjoin": "./jutil.js"
    },
    "dependencies": {
        "nomnom": "1.5.x",
        "wordwrap": ">=0.0.2"
    },
    "devDependencies": {
        "coveralls": "^2.13.1",
        "jshint": ">=0.5.5",
        "nyc": "^11.0.2"
    },
    "engines": {
        "node": ">=5.12.0"
    },
    "scripts": {
        "test": "nyc tests/run_tests",
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    }
}
